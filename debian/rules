#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

.PHONY: override_dh_strip
override_dh_strip:
	dh_strip --dbg-package=lizardfs-dbg

override_dh_auto_install:
	dh_auto_install --parallel
	cd debian/tmp/etc/mfs;ls -al;for file in *.dist; do mv "$$file" "$${file%.dist}";done

%:
	dh $@ --parallel
